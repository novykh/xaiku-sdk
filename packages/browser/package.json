{
  "name": "@xaiku/browser",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/es6/index.js",
  "jsnext:main": "dist/es6/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build:cjs": "babel src --out-dir dist --copy-files",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir dist/es6 --copy-files",
    "build:umd": "NODE_ENV=production webpack",
    "build": "pnpm run build:umd && pnpm run build:cjs && pnpm run build:es6",
    "test": "jest",
    "lint": "eslint './src/**/*.js'",
    "clean": "rimraf lib dist es coverage node_modules",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "cp-xaiku": "cp -R ./dist ../xaiku/node_modules/@xaiku/browser/",
    "to-xaiku": "pnpm run build && pnpm run cp-xaiku",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@xaiku/core": "workspace:*",
    "@xaiku/shared": "workspace:*"
  },
  "devDependencies": {
    "@babel/cli": "catalog:build",
    "@chromatic-com/storybook": "catalog:storybook",
    "@storybook/addon-essentials": "catalog:storybook",
    "@storybook/addon-interactions": "catalog:storybook",
    "@storybook/addon-webpack5-compiler-swc": "catalog:storybook",
    "@storybook/blocks": "catalog:storybook",
    "@storybook/html": "^8.5.6",
    "@storybook/html-webpack5": "^8.5.6",
    "@storybook/test": "catalog:storybook",
    "@xaiku/babel-config": "workspace:*",
    "@xaiku/eslint-config": "workspace:*",
    "@xaiku/prettier-config": "workspace:*",
    "babel-jest": "catalog:build",
    "babel-loader": "catalog:build",
    "clean-webpack-plugin": "catalog:build",
    "jest": "catalog:test",
    "jest-environment-jsdom": "catalog:test",
    "jest-mock": "catalog:test",
    "storybook": "catalog:storybook",
    "webpack": "catalog:build",
    "webpack-cli": "catalog:build"
  }
}
