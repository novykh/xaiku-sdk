{
  "name": "@xaiku/nextjs",
  "version": "0.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/novykh/xaiku-sdk.git"
  },
  "private": false,
  "main": "dist/index.js",
  "module": "dist/es6/index.js",
  "jsnext:main": "dist/es6/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build:cjs": "babel src --out-dir dist --copy-files",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir dist/es6 --copy-files",
    "build:umd": "NODE_ENV=production webpack",
    "build": "pnpm build:umd && pnpm build:cjs && pnpm build:es6",
    "test": "jest",
    "lint": "eslint './src/**/*.js'",
    "clean": "rimraf lib dist es coverage",
    "prepublishOnly": "pnpm clean && pnpm build",
    "cp-xaiku": "cp -R ./dist ../xaiku/node_modules/@xaiku/react/",
    "to-xaiku": "pnpm build && pnpm cp-xaiku",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@xaiku/node": "workspace:*",
    "@xaiku/react": "workspace:*"
  },
  "peerDependencies": {
    "next": "catalog:peers",
    "react": "catalog:peers",
    "react-dom": "catalog:peers"
  },
  "devDependencies": {
    "@babel/cli": "catalog:building",
    "@chromatic-com/storybook": "catalog:storybook",
    "@storybook/addon-essentials": "catalog:storybook",
    "@storybook/addon-interactions": "catalog:storybook",
    "@storybook/addon-onboarding": "catalog:storybook",
    "@storybook/blocks": "catalog:storybook",
    "@storybook/nextjs": "^8.5.6",
    "@storybook/test": "catalog:storybook",
    "@xaiku/babel-config": "workspace:*",
    "@xaiku/eslint-config": "workspace:*",
    "@xaiku/prettier-config": "workspace:*",
    "babel-jest": "catalog:building",
    "babel-loader": "catalog:building",
    "clean-webpack-plugin": "catalog:building",
    "jest": "catalog:test",
    "jest-environment-jsdom": "catalog:test",
    "jest-mock": "catalog:test",
    "msw": "catalog:test",
    "msw-storybook-addon": "catalog:test",
    "next": "catalog:",
    "react": "catalog:",
    "storybook": "catalog:storybook",
    "webpack": "catalog:building",
    "webpack-cli": "catalog:building"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
