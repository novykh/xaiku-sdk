{
  "name": "@xaiku/react",
  "version": "0.0.0",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/es6/index.js",
  "jsnext:main": "dist/es6/index.js",
  "files": [
    "/dist"
  ],
  "scripts": {
    "build:cjs": "babel src --out-dir dist --copy-files",
    "build:es6": "BABEL_ENV=es6 babel src --out-dir dist/es6 --copy-files",
    "build:umd": "NODE_ENV=production webpack",
    "build": "yarn build:umd && yarn build:cjs && yarn build:es6",
    "test": "jest",
    "lint": "eslint './src/**/*.js'",
    "clean": "rimraf lib dist es coverage node_modules",
    "prepublishOnly": "yarn clean && yarn build",
    "cp-xaiku": "cp -R ./dist ../xaiku/node_modules/@xaiku/react/",
    "to-xaiku": "yarn build && yarn cp-xaiku",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@xaiku/browser": "*"
  },
  "peerDependencies": {
    "react": "18.x || 19.x"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-onboarding": "^8.5.3",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/react-webpack5": "^8.5.3",
    "@storybook/test": "^8.5.3",
    "@xaiku/babel-config": "*",
    "@xaiku/eslint-config": "*",
    "@xaiku/prettier-config": "*",
    "babel-jest": "^29.7.0",
    "clean-webpack-plugin": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock": "^29.7.0",
    "msw": "^2.7.0",
    "msw-storybook-addon": "^2.0.4",
    "storybook": "^8.5.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}