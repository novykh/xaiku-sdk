{
  "name": "xaiku-sdk",
  "version": "1.0.0",
  "main": "index.js",
  "author": "novykh <ioannis.klironomos@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "pnpm run --filter '@xaiku/*' --if-present build",
    "lint": "pnpm run --filter '@xaiku/*' --if-present lint",
    "test": "pnpm run --filter '@xaiku/*' --if-present test --passWithNoTests",
    "clean": "pnpm run --filter '@xaiku/*' --if-present clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{js,mjs,md}\"",
    "prepublishOnly": "pnpm run clean && pnpm run  build",
    "release": "changeset publish"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "@xaiku/eslint-config": "workspace:*",
    "@xaiku/prettier-config": "workspace:*"
  }
}
